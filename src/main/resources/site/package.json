  "main": "index.js",
  "scripts": {
    "build:client:styles": "lessc styles/styles.less > assets/styles.css",
    "watch:client": "parallelshell \"npm run watch:client:css\" \"npm run watch:client:scripts\"",
    "watch:client:styles": "nodemon -q -w styles --ext 'less' --exec 'npm run build:client:styles'",
    "watch:client:scripts": "watchify scripts/*.js -o assets/main.js -v",
    "watch": "parallelshell \"npm run watch:client:scripts\" \"npm run watch:client:styles\""
  },
