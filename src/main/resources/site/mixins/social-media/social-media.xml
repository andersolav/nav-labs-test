<mixin>
  <display-name>Visning i sosiale medier</display-name>
  <items>
    <input type="TextLine" name="socialMediaTitle">
      <label>Tittel</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input type="TextArea" name="socialMediaIntroduction">
      <label>Ingress</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input type="ImageSelector" name="socialMediaImage">
      <label>Bilde</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
  </items>
</mixin>